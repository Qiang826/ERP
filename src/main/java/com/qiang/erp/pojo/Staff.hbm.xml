<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.qiang.erp.pojo" auto-import="true">
		<class name="Staff" table="staff">
			<id name="id" column="id">
				<generator class="native"></generator>
			</id>
			
			<property name="staff_id" column="staff_id" ></property>
			<property name="staff_name" column="staff_name"></property>
			<property name="password" column="password"></property>
			<property name="duty" column="duty"></property>
			<property name="real_name" column="real_name"></property>
			<property name="phone" column="phone"></property>
			<property name="qq" column="qq"></property>
			<property name="weixing" column="weixin"></property>
			<property name="create_time" column="create_time" type="java.util.Date"></property>
		</class>
</hibernate-mapping>